{
    "name": "ubirak/moco-behat-extension",
    "description": "Helper to use moco with behat",
    "type": "behat-extension",
    "license": "MIT",
    "homepage": "https://github.com/ubirak/moco-behat-extension",
    "authors": [
        {
            "name": "Ubirak team",
            "email": "team@ubirak.org"
        }
    ],
    "replace": {
        "rezzza/moco-behat-extension": "self.version"
    },
    "autoload": {
        "psr-4": { "Ubirak\\MocoBehatExtension\\": "src/" }
    },
    "require": {
        "php": ">=5.5.9",
        "behat/behat": "~3.0"
    },
    "require-dev": {
        "ubirak/rest-api-behat-extension": "^7.0",
        "symfony/process": "^3.0",
        "atoum/atoum": "^3.2",
        "php-http/curl-client": "^1.7",
        "guzzlehttp/psr7": "^1.4"
    },
    "bin": ["bin/install-moco.sh"]
}
